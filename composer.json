{
    "name": "fanboykun/decissionsupportsystem",
    "description": "Decission Support System's Method Calculator",
    "keywords": [
        "fanboykun",
        "decissionsupportsystem"
    ],
    "homepage": "https://github.com/fanboykun/decissionsupportsystem",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "fanboykun",
            "email": "irfanramadhan1812@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Fanboykun\\DecissionSupportSystem\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fanboykun\\DecissionSupportSystem\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fanboykun\\DecissionSupportSystem\\DecissionSupportSystemServiceProvider"
            ],
            "aliases": {
                "DecissionSupportSystem": "Fanboykun\\DecissionSupportSystem\\DecissionSupportSystemFacade"
            }
        }
    }
}
